{
  "plugins": ["plugins/markdown", "node_modules/better-docs/category"],
  "recurseDepth": 10,
  "source": {
    "include": [".", "src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs",
    "readme": "README.md",
    "template": "node_modules/better-docs"
  }
}
